   <string notr="true">/* Main Application Background */
QWidget {
    background-color: #1A1C1D; /* Almost black */
    color: #FFFFFF; /* White text */
    font-family: &quot;Consolas&quot;, &quot;Monospace&quot;;
    font-size: 14px;
}

/* Buttons */
QPushButton {
    background-color: #5CE1E6; /* Cold cyan */
    color: #1A1C1D; /* Dark text on bright button */
    border: 1px solid #5CE1E6;
    border-radius: 6px;
    padding: 6px 12px;
}

QPushButton:hover {
    background-color: #8CE3F5; /* Light neon on hover */
}

QPushButton:pressed {
    background-color: #00B4D8; /* Stronger cyan on press */
}

/* Text Inputs */
QLineEdit, QTextEdit {
    background-color: #2E3233; /* Dark panel */
    color: #FFFFFF;
    border: 1px solid #5CE1E6;
    border-radius: 4px;
    padding: 5px;
}

QLineEdit:focus, QTextEdit:focus {
    border: 1px solid #00B4D8; /* Glow cyan focus */
}

/* Labels */
QLabel {
    color: #8CE3F5;
}

/* Scrollbars */
QScrollBar:vertical, QScrollBar:horizontal {
    background-color: #2E3233;
}

QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background-color: #5CE1E6;
    border-radius: 6px;
}

/* Menus */
QMenuBar {
    background-color: #1A1C1D;
    color: #FFFFFF;
}

QMenuBar::item:selected {
    background-color: #00B4D8;
}

QMenu {
    background-color: #2E3233;
    color: #FFFFFF;
    border: 1px solid #5CE1E6;
}

QMenu::item:selected {
    background-color: #5CE1E6;
    color: #1A1C1D;
}

/* Tabs */
QTabWidget::pane {
    background: #2E3233;
    border: 1px solid #5CE1E6;
}

QTabBar::tab {
    background: #2E3233;
    color: #8CE3F5;
    padding: 8px 12px;
    border: 1px solid #5CE1E6;
    border-bottom: none;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

QTabBar::tab:selected {
    background: #1A1C1D;
}

QTabBar::tab:hover {
    background: #5CE1E6;
    color: #1A1C1D;
}

/* Sliders */
QSlider::groove:horizontal {
    background: #5A595C;
    height: 6px;
    border-radius: 3px;
}

QSlider::handle:horizontal {
    background: #5CE1E6;
    width: 16px;
    height: 16px;
    margin: -5px 0;
    border-radius: 8px;
}

/* Progress Bars */
QProgressBar {
    background-color: #2E3233;
    border: 1px solid #5CE1E6;
    border-radius: 4px;
    color: #FFFFFF;
}

QProgressBar::chunk {
    background-color: #5CE1E6;
    border-radius: 4px;
}

/* Group Boxes */
QGroupBox {
    border: 1px solid #5CE1E6;
    border-radius: 4px;
    margin-top: 10px;
    padding-top: 10px;
    color: #8CE3F5;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 0 5px;
    background-color: #1A1C1D;
}</string>
